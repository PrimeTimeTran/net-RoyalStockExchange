DROP TABLE [dbo].[Stocks];

create table [dbo].[Stocks] (
    [Id] INT PRIMARY KEY IDENTITY(1, 1),
    [Name] [nvarchar](30),
    [Price] DECIMAL(18, 2),
    [Quantity] [int],
);
GO

INSERT [dbo].[Stocks]([Name], [Price], [Quantity])
VALUES('Z', 100.01, 100);
INSERT [dbo].[Stocks]([Name], [Price], [Quantity])
VALUES('Y', 200.02, 2000);
INSERT [dbo].[Stocks]([Name], [Price], [Quantity])
VALUES('X', 300.03, 3000);
GO